<UserControl x:Class="FESScript2.UserControls.SubUserControls.TextBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FESScript2.UserControls.SubUserControls"
             xmlns:UserControls="clr-namespace:FESScript2.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="150" IsManipulationEnabled="True" MouseDown="UserControl_MouseDown">
    <Grid Height="25" Width="auto">
        <local:BlockBackground Fill="White" Stroke="Gray" StrokeThickness="2"/>
        <TextBox MinWidth="25" Height="15" MaxLines="1" x:Name="textBox" Margin="7.5, 5" VerticalAlignment="Center" FontSize="12.5" FontFamily="Times New Roman" BorderThickness="0" IsEnabled="False"></TextBox>
    </Grid>
</UserControl>
